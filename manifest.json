{
  "manifest_version": 2,
  "name": "Make Twitter Square Again!",
  "description": "Make Twitter Avatars Square Again!",
  "version": "1.0.1",
  "author": "inventivetalent",
  "browser_action": {
    "default_icon": "img/Twitter_Logo_White_On_Blue-32.png"
  },
  "icons": {
    "32": "img/Twitter_Logo_White_On_Blue-32.png",
    "64": "img/Twitter_Logo_White_On_Blue-64.png",
    "128": "img/Twitter_Logo_White_On_Blue-128.png",
    "256": "img/Twitter_Logo_White_On_Blue-256.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*",
        "http://*.twitter.com/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "key": ""
}